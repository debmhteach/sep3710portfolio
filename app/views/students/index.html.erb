<p style="color: green"><%= notice %></p>

<h1>Students</h1>

<%= button_tag "Test Bootstrap Button", class: "btn btn-primary" %>

<div id="students">
  <% @students.each do |student| %>
    <%= render student %>
    <p>
      <%= link_to "Show this student", student %>
    </p>
  <% end %>
</div>

<%= link_to "New student", new_student_path %>
